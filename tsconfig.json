{
    "compilerOptions": {
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ES2021"
        ],
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "strictNullChecks": true,
        "jsx": "react",
        "declaration": true,
        "emitDeclarationOnly": true,
        "noImplicitReturns": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "baseUrl": ".",
    }
}